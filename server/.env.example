# MongoDB Connection
MONGODB_URI=mongodb+srv://your_username:your_password@cluster.mongodb.net/neofin?retryWrites=true&w=majority

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API Key (Optional - for using multiple models like gemini-2.0, claude, etc.)
OPENROUTER_API_KEY=sk-or-your-key-here

# Server Port
PORT=5000
